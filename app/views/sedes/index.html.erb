<p id="notice"><%= notice %></p>

<h1>Listing Sedes</h1>

<table>
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sedes.each do |sede| %>
      <tr>
        <td><%= sede.codigo %></td>
        <td><%= sede.nombre %></td>
        <td><%= sede.descripcion %></td>
        <td><%= link_to 'Show', sede %></td>
        <td><%= link_to 'Edit', edit_sede_path(sede) %></td>
        <td><%= link_to 'Destroy', sede, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sede', new_sede_path %>
